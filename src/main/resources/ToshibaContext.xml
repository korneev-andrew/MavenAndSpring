<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true"
       default-autowire-candidates="false">

    <!-- Sony Parts -->
    <bean id="sonyHand" class="impls.parts.sony.SonyHand" scope="prototype"/>
    <bean id="sonyaLeg" class="impls.parts.sony.SonyLeg" scope="prototype"/>
    <bean id="sonyHead" class="impls.parts.sony.SonyHead" scope="prototype"/>

</beans>